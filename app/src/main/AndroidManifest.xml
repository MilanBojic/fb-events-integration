<?xml version="1.0" encoding="utf-8"?>
<manifest package="fbintegration.app.com"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="fbintegration.app.com.Provisioning"
            android:exported="true"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="fbintegration.app.com.SessionDataActivity"
            android:theme="@style/AppTheme">
        </activity>

        <activity
            android:name="fbintegration.app.com.EventsDetailActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        </activity>

        <activity
            android:name="fbintegration.app.com.SearchActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />
        <provider
            android:name="com.facebook.FacebookContentProvider"
            android:authorities="com.facebook.app.FacebookContentProvider1794631367482701"
            android:exported="true" />
    </application>

</manifest>
